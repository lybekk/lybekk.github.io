    "dependencies": {
        "@fluentui/react": "^7.147.1",
        "@fortawesome/fontawesome-svg-core": "^1.2.34",
        "@fortawesome/free-brands-svg-icons": "^5.15.2",
        "@fortawesome/free-solid-svg-icons": "^5.15.2",
        "@primer/octicons": "^11.2.0",
        "@uifabric/fluent-theme": "^7.3.29",
        "bootstrap-icons": "^1.3.0",
        "clean-css": "^4.2.3",
        "file-saver": "^2.0.2",
        "lodash": "^4.17.20",
        "luxon": "^1.25.0",
        "moment": "^2.29.1",
        "node-fetch": "^2.6.1",
        "prismjs": "^1.22.0",
        "remark": "^13.0.0"
    },
    "devDependencies": {
        "@11ty/eleventy": "^0.11.1",
        "@11ty/eleventy-plugin-rss": "^1.1.0",
        "cypress": "^4.12.1",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^6.13.0",
        "eslint-plugin-prettier": "^3.1.4",
        "gh-pages": "^3.1.0",
        "jest": "^26.6.3",
        "lighthouse": "^6.4.1",
        "prettier": "2.1.1"
    },
    "keywords": [
        "eleventy"
    ],
    "license": "MIT",
    "scripts": {
        "start": "npm run serve",
        "build": "npx @11ty/eleventy",
        "serve": "npx @11ty/eleventy --serve",
        "watch": "npx @11ty/eleventy --watch",
        "debug": "DEBUG=* eleventy",
        "deploy": "npx @11ty/eleventy && npx gh-pages -d _site -b main -r https://github.com/lybekk/lybekk.github.io",
        "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
        "test": "jest",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
        "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
        "lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js",
        "build:docs": "typedoc --ignoreCompilerErrors",
        "build:docs:ignore": "typedoc --ignoreCompilerErrors",
        "11tyhelp": "npx @11ty/eleventy --help"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lybekk/lybekk.github.io"
    },
    "bugs": {
        "url": "https://github.com/lybekk/lybekk.github.io/issues"
    }
